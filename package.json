{
  "name": "digital-library-frontend",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js", 
  "homepage": "./",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "adm-zip": "^0.5.16",
    "axios": "^1.13.4",
    "framer-motion": "^12.23.24",
    "jszip": "^3.10.1",
    "localforage": "^1.10.0",
    "lucide-react": "^0.553.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^6.14.1",
    "react-scripts": "5.0.1",
    "react-tsparticles": "^2.12.2",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:pack": "npm run build && electron-builder -w -c.extraMetadata.main=build/electron.js"
  },
  "build": {
    "appId": "com.digital.library",
    "productName": "Thu Vien So BKAP",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "public",
      "output": "dist"
    },
    "win": {
      "target": "nsis",
      "icon": "public/favicon.ico"
    }
  },
  "devDependencies": {
    "concurrently": "^9.0.0",
    "cross-env": "^7.0.0",
    "electron": "^32.0.0",
    "electron-builder": "^24.0.0",
    "wait-on": "^7.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
